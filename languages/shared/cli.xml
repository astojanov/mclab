<!--
    Requires:
        Properties: ant.bin.dir, test.suite.name
        Paths: junit.jar.path, beaver.rt.jar.path
        Targets: eclipse.build, compile, eclipse.clean, clean.compile, eclipse.test
-->
<project name="CommandLineInterface">
	<!-- Command-line targets -->
	<target name="build" depends="eclipse.build, compile" description="generate and build all files" />

	<target name="clean" depends="eclipse.clean, clean.compile" description="deletes all files produced by build" />

	<target name="test" depends="build, eclipse.test" description="runs all JUnit tests" />
</project>